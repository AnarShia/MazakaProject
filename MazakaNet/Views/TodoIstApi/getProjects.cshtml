@model IEnumerable<Todoist.Net.Models.Project>


Total Resource: @Model.Count()



    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Collapsed</th>
            <th scope="col">Color</th>
            <th scope="col">Shared</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var project in @Model)
        {
            <tr>
                <th scope="row">@project.Id</th>
                <td>@project.Name</td>
                <td>@project.Collapsed</td>
                <td>@project.Color</td>
                <td>@project.Shared</td>

            </tr>
        }
        </tbody>
    </table>
